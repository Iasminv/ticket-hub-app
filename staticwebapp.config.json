{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/*.html"
    },
    {
      "route": "/404",
      "serve": "/404.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "platformErrorOverrides": [
    {
      "errorType": "NotFound",
      "serve": "/404.html"
    }
  ]
}